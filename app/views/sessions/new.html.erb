<%= provide(:title,'Sign In') %> <h1>Sign In</h1><div class="row">	<div class="col-lg-6 col-lg-offset-3">		<%= form_for(:session, url: sessions_path) do |f|%>			<%= f.label :email, "Email" %>			<%= f.text_field :email %>				<%= f.label :password, "Password" %>			<%= f.password_field :password %>				<%= f.submit "Sign in", class: "btn btn-large btn-primary" %>		<% end%>		<p>New User? <%= link_to('Sign up here', signup_path) %></p>		</div></div>